[
  {
    "bindings": {
      "ctrl-shift-o": "file_finder::Toggle",
      "ctrl-shift-e": "workspace::ToggleRightDock",
      "ctrl-shift-q": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-d": "editor::DeleteLine",
      "alt-m": "outline::Toggle",
      "ctrl-b": "editor::GoToDefinition"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-alt-0": null,
      "ctrl-shift-b": [ "task::Spawn", { "task_name": "Build Shaders" } ]
    }
  },
  {
    "context": "AgentPanel",
    "bindings": {
      "ctrl-alt-/": null
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-*": [
        "editor::ToggleComments",
        {
          "advance_downwards": false
        }
      ]
    }
  }
]
